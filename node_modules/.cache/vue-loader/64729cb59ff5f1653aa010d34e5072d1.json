     </b-form-group>\n                \n                <b-form-group \n                label=\"Password\" \n                >\n                  <b-form-input\n                    type=\"password\"\n                    v-model=\"password\"\n                    required\n                    placeholder=\"Enter Password\"\n                  ></b-form-input>\n                </b-form-group>\n                <button  @click=\"signin()\" style=\" background-color: Transparent; color:white; font-size:20px;\">Login</button>\n              \n              \n            </div>\n          </div>\n        </b-col>\n      </b-row>\n    </b-container>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport { ipBackend } from \"@/config.js\";\nexport default {\n    name: \"login\",\n    data (){\n        return{\n        username: '',\n        password: ''\n        \n    };\n  },\n  methods: {\n        signin(){\n            let vm = this;\n            axios.post(ipBackend + '/karyawan/login', {\n                username:vm.username,\n                password:vm.password,\n                \n            })\n            .then(res => {\n              console.log(res.data[0], res.data[1])\n              localStorage.setItem('token',res.data[0])\n              if(res.data.length){  \n                alert(\"Selamat anda telah login\");\n                vm.$router.push({ path: \"/dashboardadmin\" });\n              }\n              else{\n                alert(res.data.message)\n              }\n            })\n            .catch(err => {\n                console.log(err, 'ini eror')\n            })\n        }\n    }\n}\n</script>\n\n<style>\n\n</style>"]}]}